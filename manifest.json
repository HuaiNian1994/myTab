{
    "name": "myTab",
    "description": "Override the new tab page with MINE",
    "version": "0.1",
    "browser_action": {
	"default_icon": "images/myIcon.png",
	"default_popup": "popup.html"
	},
	"icons":{
		"128":"images/myIcon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://newtab/*"
			],

			"js": [
				"js/myContentScript.js"
			],
			"run_at": "document_end"
		}
	],
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	},
    "chrome_url_overrides": {
        "newtab": "myIndex.html"
	},
	 "permissions": [
		 "activeTab",
		 "storage"
	 ],
    "manifest_version": 2
}
